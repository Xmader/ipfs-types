{
    "compilerOptions": {
        "module": "commonjs",
        "lib": [
            "es2019",
            "DOM"
        ],
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictFunctionTypes": true,
        "strictNullChecks": true,
        "esModuleInterop": true,
        "baseUrl": "../",
        "typeRoots": [
            "../"
        ],
        "types": [
            "node"
        ],
        "noEmit": true,
        "forceConsistentCasingInFileNames": true
    },
}
